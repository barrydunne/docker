{
  "AllowedHosts": "*",
  "SecretsManagerOptions": {
    "BaseUrl": "http://secretsmanager:8080"
  },
  "RabbitMQQueueOptions": {
    "Nodes": [ "rabbitmq1:5672", "rabbitmq2:5672" ],
    "User": "<secret>",
    "Password": "<secret>",
    "VirtualHost": "<secret>",
    "SubscriberSuffix": "Email",
    "RetryDelayMilliseconds": 30000
  },
  "SmtpEmailOptions": {
    "From": "microservices-notifications@example.com",
    "Host": "cloudemail",
    "Port": 1025
  },
  "FtpFilesOptions": {
    "Host": "cloudfiles",
    "Port": 21,
    "BaseDir": "/files"
  },
  "ConnectionStrings": {
    "mysql": "<secret>"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Debug", "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Email"
    }
  }
}